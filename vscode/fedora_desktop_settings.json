{
  "extensions.ignoreRecommendations": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "phase",
  "editor.fontLigatures": true,
  "editor.fontFamily": "'Jetbrains Mono Nerd Font', 'Jetbrains Mono', 'Comic Code Ligatures',  'Fira Code', 'Iosevka Term', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'" /*
    "vscode_custom_css.imports": [
        "file:///home/vestue/.vscode/extensions/thecodemonkey.synthwave-x-fluoromachine-epic-animations-1.4.13/synthwave-x-fluoromachine.css",
        "file:///home/vestue/.vscode/extensions/thecodemonkey.synthwave-x-fluoromachine-epic-animations-1.4.13/epic-80s-transitions.css"
        ],*/,
  "editor.fontSize": 12,
  //"editor.fontWeight": "bold",
"keyboard.dispatch": "keyCode",
  "security.workspace.trust.untrustedFiles": "open",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "newLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.type": "newLine",
  "C_Cpp.autocompleteAddParentheses": true,
  "C_Cpp.vcFormat.space.beforeOpenSquareBracket": true,
  "C_Cpp.vcFormat.space.betweenEmptyLambdaBrackets": true,
  "C_Cpp.vcFormat.space.betweenEmptySquareBrackets": true,
  "C_Cpp.vcFormat.newLine.scopeBracesOnSeparateLines": true,
  "C_Cpp.vcFormat.space.betweenEmptyBraces": true,
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: LLVM, UseTab: Never, IndentWidth: 4, TabWidth: 4, BreakBeforeBraces: Attach, AllowShortIfStatementsOnASingleLine: false, IndentCaseLabels: false, ColumnLimit: 0, AccessModifierOffset: -4 }",
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "editor.formatOnType": false,
  "better-comments.multilineComments": true,
  "editor.detectIndentation": false,
  "workbench.editor.showTabs": "single",
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "git.autofetch": true,
  "files.autoSave": "afterDelay",
  "latex-workshop.message.update.show": false,
  "latex-workshop.message.warning.show": false,
  "latex-workshop.message.information.show": false,
  "latex-workshop.message.error.show": false,
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.build.forceRecipeUsage": false,
  "editor.inlineSuggest.enabled": true,
  "git.confirmSync": false,
  "github.copilot.enable": {
    "*": false,
    "yaml": false,
    "plaintext": false,
    "markdown": false,
    "sqlite": false
  },
  "cmake.configureOnOpen": false,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "[fsharp]": {
    "editor.defaultFormatter": "Ionide.Ionide-fsharp"
  },
  "window.zoomLevel": 1,
  "editor.minimap.enabled": false,
  "workbench.startupEditor": "none",
  "redhat.telemetry.enabled": true,
  "editor.formatOnSave": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "black-formatter.args": ["--line-length", "100"],
  "git.openRepositoryInParentFolders": "never",
  "workbench.colorTheme": "Catppuccin Noctis Mocha",
  "editor.formatOnPaste": true,
